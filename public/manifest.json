{
    "manifest_version": 3,
    "name": "Naitik",
    "version": "1.0.0",
    "description": "A basic extension to battle dark patterns",
    "icons": {
        "32": "images/icon32.png"
      },
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "https://*/*",
        "http://*/*"
    ]

}  